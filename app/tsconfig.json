{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
		"baseUrl": "./",
		"paths": {
			"@components/*": [
				"./src/components/*"
			],
      "@assets/*": [
        "./src/assets/*"
      ],
			"@screens/*": [
				"./src/screens/*"
			],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@themes/*": [
        "./src/themes/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  }
}